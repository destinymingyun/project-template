<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xingchenproject.template.projecttemplate.mapper.DictMapper">
    <insert id="postDict" useGeneratedKeys="true" keyProperty="dictId">
        INSERT INTO `dict` (`dict_name`, `dict_name`)
        VALUES (#{dictName}, #{dictDesc});
    </insert>
</mapper>